    <Template>4</Template>
    <Content x="280" y="35" width="600" height="120" hasBackground="false" bgColor="#FFFFFF">&lt;P ALIGN="LEFT"&gt;&lt;FONT SIZE="24" COLOR="#000000" LETTERSPACING="0" KERNING="0"&gt;Masks are crucial for those looking after someone who is ill or for the patient to prevent infection. &lt;/FONT&gt;&lt;/P&gt;</Content>
    <Narrator x="-100" y="200" start="0" stop="400">sound/COVID_601.mp3</Narrator>
    <Image x="367" y="77" width="400" height="400">swf/Element_icon_mask.swf</Image>
